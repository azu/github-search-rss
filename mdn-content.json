        },
        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/87750369?u=ccc1f52005708ae16bcb50e325e02145bff0ef7d&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/><ul dir=\"auto\">\n<li>continuing <a class=\"issue-link js-issue-link\" data-error-text=\"Failed to load title\" data-id=\"2166125211\" data-permission-text=\"Title is private\" data-url=\"https://github.com/mdn/content/issues/32539\" data-hovercard-type=\"pull_request\" data-hovercard-url=\"/mdn/content/pull/32539/hovercard\" href=\"https://github.com/mdn/content/pull/32539\">#32539</a></li>\n</ul>",
            "url": "https://github.com/mdn/content/pull/32542",
