            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/33548838?u=ea609280f61608dd20c0388e0a41c695cec12dd0&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/>",
            "url": "https://github.com/mdn/content/pull/27019",
