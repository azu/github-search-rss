        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/59933391?u=29a21d8e4c83c4677c0b52554e077f7eb792bf45&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/><p dir=\"auto\">Change Selector group term to be selector list</p>",
            "url": "https://github.com/mdn/content/pull/29559",
            "title": "Change Selector group term to be selector list",
