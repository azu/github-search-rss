        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/1297909?v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/>",
            "url": "https://github.com/mdn/content/pull/22012",
            "title": "fix: various typos",
            "date_modified": "2022-11-07T10:17:51.000Z",
            "date_published": "2022-11-02T20:37:22.000Z",
            "author": {
                "name": "nschonni",
