        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/44072237?u=b3da8cad9b531a002a897473993d6847d966a1ae&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/>\n<h4 dir=\"auto\">Summary</h4>\n\n<p dir=\"auto\">We're enabling <code class=\"notranslate\">fetchpriority</code> by default in Fx 132</p>\n<h4 dir=\"auto\">Test results and supporting details</h4>\n\n\n<p dir=\"auto\"><a href=\"https://bugzilla.mozilla.org/show_bug.cgi?id=1854077\" rel=\"nofollow\">https://bugzilla.mozilla.org/show_bug.cgi?id=1854077</a></p>\n<h4 dir=\"auto\">Related issues</h4>\n\n",
            "url": "https://github.com/mdn/browser-compat-data/pull/24518",
