        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/5179191?u=6717e5ff89d22ac777c8bebbf54e89ed0fbf81e8&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/><p dir=\"auto\">This PR replaces all double spaces from our notes.</p>",
            "url": "https://github.com/mdn/browser-compat-data/pull/15992",
