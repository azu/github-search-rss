        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/6173598?u=7b13815898f48600ed407ca5d6ea82645d499ebd&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/><h4 dir=\"auto\">Summary</h4>\n<p dir=\"auto\">Chrome added support for supportedValues of in v99</p>\n<h4 dir=\"auto\">Test results and supporting details</h4>\n<p dir=\"auto\">Tested in console in my browser.</p>\n\n<p dir=\"auto\">See: <a href=\"https://chromestatus.com/feature/5649454590853120\" rel=\"nofollow\">https://chromestatus.com/feature/5649454590853120</a></p>",
            "url": "https://github.com/mdn/browser-compat-data/pull/17142",
