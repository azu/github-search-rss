        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/2877584?u=193f7215d9ae0d611e2caeed235a0006fa0eeb64&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/><h4 dir=\"auto\">Summary</h4>\n<p dir=\"auto\">Changed initial support of public static class methods for Safari</p>\n<h4 dir=\"auto\">Test results and supporting details</h4>\n<p dir=\"auto\"><a href=\"https://live.browserstack.com/dashboard#os=iOS&amp;os_version=9.0&amp;device_browser=safari&amp;zoom_to_fit=true&amp;full_screen=true&amp;url=https%3A%2F%2Fcodepen.io%2FPonomareVlad%2Fembed%2FgOoXGRx%3Fdefault-tab%3Djs%252Cresult&amp;speed=1&amp;start=true\" rel=\"nofollow\">Test on iOS 9</a></p>\n<p dir=\"auto\"><a href=\"https://live.browserstack.com/dashboard#os=OS+X&amp;os_version=El+Capitan&amp;browser=Safari&amp;browser_version=9.0&amp;device_browser=safari&amp;zoom_to_fit=true&amp;full_screen=true&amp;resolution=responsive-mode&amp;url=https%3A%2F%2Fcodepen.io%2FPonomareVlad%2Fembed%2FgOoXGRx%3Fdefault-tab%3Djs%252Cresult&amp;speed=1&amp;start=true\" rel=\"nofollow\">Test on MacOS El Capitan</a></p>",
