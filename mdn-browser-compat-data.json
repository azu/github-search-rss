        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/1418446?u=c3134e9d57cd0113af51f14bcf2e996d52da488c&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/><h4 dir=\"auto\">Summary</h4>\n<p dir=\"auto\">RegExp.escape added to nodejs v24</p>\n<h4 dir=\"auto\">Test results and supporting details</h4>\n<p dir=\"auto\"><a href=\"https://nodejs.org/en/blog/release/v24.0.0\" rel=\"nofollow\">https://nodejs.org/en/blog/release/v24.0.0</a></p>",
            "url": "https://github.com/mdn/browser-compat-data/pull/26735",
