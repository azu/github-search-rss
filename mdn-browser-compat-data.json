        {
            "content_html": "<img src=\"https://avatars.githubusercontent.com/u/349114?u=de6bd3d5d570d97e4c15ba2b0e774f3ebd2456ae&v=4\" width=\"64\" height=\"64\" alt=\"\"/><br/><p dir=\"auto\">Found by the Open Web Docs BCD collector v10.6.3.</p>\n<p dir=\"auto\">Not in Chromium, confirmed by <a href=\"https://bugs.chromium.org/p/chromium/issues/detail?id=707640\" rel=\"nofollow\">https://bugs.chromium.org/p/chromium/issues/detail?id=707640</a></p>",
            "url": "https://github.com/mdn/browser-compat-data/pull/21643",
